{
  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "probe-rs-debugger-test",
      "type": "probe-rs-debug",
      "request": "launch",
      "name": "probe-rs-debugger-test",
      "chip": "${config:probe-rs-chip}",
      "coreConfigs": [
        {
          "coreIndex": 0,
          "programBinary": "./target/${config:rust-analyzer.cargo.target}/${config:probe-rs-profile}/${workspaceFolderBasename}",
          "svdFile": "./svd_files/${config:probe-rs-chip}.svd",
          "rttEnabled": true,
          "rttChannelFormats": [
            {
              "dataFormat": "String",
              "channelNumber": 0,
              "showTimestamps": false
            },
            {
              "dataFormat": "BinaryLE",
              "channelNumber": 1
            }
          ]
        }
      ],
      "flashingConfig": {
        "flashingEnabled": true,
        "haltAfterReset": false
      },
      // "server": "127.0.0.1:50001", // OPTIONAL: For connecting to an existing probe-rs-debugger process.
      // "consoleLogLevel": "Debug",
      // "env": {
      // "RUST_LOG": "debug",
      // "DEFMT_LOG": "debug"
      // },
      "speed": 4000 // CONFIGURE: Optionally update USB speed
    }
    ]
}
